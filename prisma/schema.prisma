generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  externalUserId String @unique
  username String @unique
  profile String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}